<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/imgiv"
    android:orientation="vertical"
    tools:context=".Info_admin">



    <Button
        android:id="@+id/button2"
        android:layout_width="125dp"
        android:layout_height="50dp"
        android:background="@color/principalbg"
        android:fontFamily="casual"
        android:text="@string/inicio"
        android:textColor="@color/principaltxt"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/button3"
        android:layout_width="142dp"
        android:layout_height="50dp"
        android:background="@color/principalbg"
        android:fontFamily="casual"
        android:text="@string/videollamada"
        android:textColor="@color/principaltxt"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />


    <Button
        android:id="@+id/button4"
        android:layout_width="126dp"
        android:layout_height="50dp"
        android:background="@color/principalbg"
        android:fontFamily="casual"
        android:text="@string/contactos"
        android:textColor="@color/principaltxt"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/button3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/infoa2"
        android:layout_width="250dp"
        android:layout_height="54dp"
        android:background="@color/principaltxt"
        android:fontFamily="casual"
        android:paddingTop="15dp"
        android:text="@string/texto_de_prueba_01"
        android:textAlignment="center"
        android:textColor="@color/principalbg"
        app:layout_constraintBottom_toBottomOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/infoa3"
        app:layout_constraintVertical_bias="0.038" />

    <TextView
        android:id="@+id/infoa3"
        android:layout_width="250dp"
        android:layout_height="54dp"
        android:background="@color/principalbg"
        android:fontFamily="casual"
        android:paddingTop="15dp"
        android:text="@string/texto_de_prueba_02"
        android:textAlignment="center"
        android:textColor="@color/principaltxt"
        app:layout_constraintBottom_toBottomOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.483" />

    <TextView
        android:id="@+id/infoa4"
        android:layout_width="215dp"
        android:layout_height="50dp"
        android:background="@color/principaltxt"
        android:fontFamily="casual"
        android:paddingLeft="10dp"
        android:paddingTop="15dp"
        android:text="@string/Usuarioinfa"
        android:textColor="@color/principalbg"
        app:layout_constraintBottom_toTopOf="@+id/infoa3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.398"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.797" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="110dp"
        android:layout_height="107dp"
        app:layout_constraintBottom_toTopOf="@+id/infoa4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.183"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_launcher_foreground" />


</androidx.constraintlayout.widget.ConstraintLayout>

